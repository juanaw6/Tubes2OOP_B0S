<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="CENTER" stylesheets="@main.css" prefHeight="130.0" prefWidth="110.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141" style="-fx-background-color: #BBE2EC; -fx-border-color: #1B425B; -fx-border-width: 3px; -fx-background-radius: 10px; -fx-border-radius: 8px;" fx:controller="app.CardController" fx:id="card">
   <children>
      <ImageView fitHeight="90.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true" fx:id="cardImage">
         <image>
            <Image url="@../public/Accelerate.png" fx:id="ImageView"/>
         </image>
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </ImageView>
      <Label text="clock" fx:id="cardLabel" style="-fx-font-family: 'Inter'; -fx-font-size: 14px; -fx-font-weight: 700;">
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
      </Label>
   </children>
</VBox>